<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings>
  <dl_settings>
     <dl_settings NAME="Att GainSet">
	<dl_setting var="activeGainSet" min="1" step="1" max="3" module="modules/ATMOS/multiGain" shortname="gainSet" values="SET A|SET B|SET C" handler="SetGainSetHandler">
		<strip_button name="GAIN A" icon="gainA.png" value="1"/>
		<strip_button name="GAIN B" icon="gainB.png" value="2"/>
		<strip_button name="GAIN C" icon="gainC.png" value="3"/>
         </dl_setting>
     </dl_settings>
     <dl_settings NAME="[A] Att Gains A">
      <dl_setting var="stabilization_gainsA.p.x" min="0" step="1" max="6000"   module="modules/ATMOS/multiGain" shortname="pgain phi" />
      <dl_setting var="stabilization_gainsA.d.x" min="0" step="1" max="6000"   module="modules/ATMOS/multiGain" shortname="dgain p"/>
      <dl_setting var="stabilization_gainsA.i.x" min="0"  step="1" max="50"    module="modules/ATMOS/multiGain" shortname="igain phi" handler="SetKiPhi"/>
      <dl_setting var="stabilization_gainsA.dd.x" min="0"    step="1" max="500" module="modules/ATMOS/multiGain" shortname="ddgain p"/>
      <dl_setting var="stabilization_gainsA.aero_activation.x" min="0"    step="1" max="1024" module="modules/ATMOS/multiGain" shortname="p aeroAct"/>

      <dl_setting var="stabilization_gainsA.p.y" min="0" step="1" max="6000"   module="modules/ATMOS/multiGain" shortname="pgain theta"/>
      <dl_setting var="stabilization_gainsA.d.y" min="0" step="1" max="6000"   module="modules/ATMOS/multiGain" shortname="dgain q"/>
