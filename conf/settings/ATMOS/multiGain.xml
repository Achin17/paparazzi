      <dl_setting var="stabilization_gainsA.aero_activation.y" min="0"    step="1" max="1024" module="modules/ATMOS/multiGain" shortname="q aeroAct"/>

      <dl_setting var="stabilization_gainsA.p.z" min="0" step="1" max="8000"   module="modules/ATMOS/multiGain" shortname="pgain psi"/>
      <dl_setting var="stabilization_gainsA.d.z" min="0" step="1" max="8000"   module="modules/ATMOS/multiGain" shortname="dgain r"/>
      <dl_setting var="stabilization_gainsA.i.z" min="0"  step="1" max="80"    module="modules/ATMOS/multiGain" shortname="igain psi"/>
