<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings>
  <dl_settings>
    <dl_settings NAME="ATMOS">
   	    <dl_setting var="transition_percentage" min="0" step="1" max="100" module="firmwares/rotorcraft/force_allocation_laws" shortname="Transition pct"/>
	    <dl_setting var="force_allocation_fixedwing_max_climb" min="0" step="0.5" max="10"  shortname="Max Climb"/>
	    <dl_setting var="force_allocation_fixedwing_pitch_of_vz" min="0" step="0.02" max="1"  shortname="Pitch of VZ"/>
  	    <dl_setting var="force_allocation_fixedwing_throttle_of_vz" min="0" step="0.02" max="1" shortname="Throttle of VZ"/>
   	    <dl_setting var="force_allocation_fixedwing_pitch_trim" min="-110" step="1" max="110"  shortname="Fixedwing pitch trim" unit="rad" alt_unit="deg"/>
   	    <dl_setting var="force_allocation_fixedwing_throttle_of_xdd" min="-1.0" step="0.01" max="1.0"  shortname="Fixedwing Throttle of XDD" />
   	    <dl_setting var="force_allocation_fixedwing_yawrate_of_ydd" min="-1.0" step="0.01" max="1.0"  shortname="Fixedwing Yawrate of ydd" />
   	    <dl_setting var="guidance_v_nominal_throttle" min="0.1" step="0.01" max="0.9" module="guidance/guidance_v" shortname="Nominal Throttle" handler="SetNominalHoverThrottle" alt_unit="%" alt_unit_coef="0.000104167"/>



      <dl_setting var="killHoverProps" min="0" step="1" max="1" module="modules/ATMOS/hoverPropsOff" shortname="hprops_off" values="OFF|ON">
	<strip_button name="HOVERPR_ON" icon="hoverpr_on.png" value="0"/>
	<strip_button name="HOVERPR_OFF" icon="hoverpr_off.png" value="1"/>
      </dl_setting>



      <dl_setting var="transition_setting" min="0" step="1" max="2" module="modules/ATMOS/newTransition" shortname="req_tr_state" values="HOVER|FORWARD|!!MAYDAY HOVER NOW!!" handler="doTransition">
		<strip_button name="TR_FW" icon="transition_forward.png" value="1"/>
		<strip_button name="TR_HO" icon="transition_hover.png" value="0"/>
		<strip_button name="TR_HOE" icon="transition_hover_emerg.png" value="2"/>
		<strip_button name="TR_HALF" value="3"/>
      </dl_setting>
      <dl_setting var="thrustActivationRatio" min="0" step="1" max="1024" module="modules/ATMOS/newTransition" shortname="Thrust Ratio" handler="ThrustActivationRatioSet"/>
    </dl_settings>
  </dl_settings>
</settings>

