<!DOCTYPE airframe SYSTEM "../../airframe.dtd">

<!--

BEBOP with bumpers, passing through the window

-->

<airframe>
  <section name="STABILIZATION_ATTITUDE_INDI" prefix="STABILIZATION_INDI_">
    <!-- bebop (no legs, with bumpers) -->
    <define name="G1_P" value="0.066554"/>
    <define name="G1_Q" value="0.034597"/>
    <define name="G1_R" value="0.002595"/>
    <define name="G2_R" value="0.136735"/>
  </section>
  
  <modules main_freq="512">
    <module name="sonar_bebop"/>
  </modules>
  
  <firmware name="rotorcraft">
    <define name="RANGE_ORIENTATION" value="1"/>
    <define name="RANGE_FRONT" value="2"/>
    <define name="RANGE_LEFT" value="3"/>
    <define name="RANGE_BACK" value="0"/>
    <define name="RANGE_RIGHT" value="1"/>
    <define name="RANGE_DOWN" value="4"/>
  </firmware>
  
  <section name="GUIDANCE_H" prefix="GUIDANCE_H_">
   <define name="REF_MAX_SPEED" value="0.3" unit="m/s"/>
 </section>
</airframe>
