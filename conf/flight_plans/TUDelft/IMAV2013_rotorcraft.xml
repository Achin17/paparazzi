    <waypoint name="S4" x="-290.5" y="152.0"/>
    <waypoint lat="43.44995675" lon="1.26320972" name="ARCH_EAST"/>
    <waypoint lat="43.4495515" lon="1.26431293" name="ARCH_WEST"/>
    <waypoint lat="43.44895808" lon="1.26539215" name="VTOL"/>
    <waypoint lat="43.44865401" lon="1.26603716" name="DZ1"/>
