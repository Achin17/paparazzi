<!DOCTYPE flight_plan SYSTEM "../../flight_plan.dtd">

<flight_plan alt="80" ground_alt="0" home_mode_height="80" lat0="52.1706181" lon0="4.4287684" max_dist_from_home="1400" name="VALK2018" qfu="20" security_height="10">
  <header>
#include "autopilot.h"
#include "guidance/guidance_delftacopter.h"
#include "guidance/guidance_v.h"
#include "guidance/guidance_h.h"
#include "subsystems/electrical.h"
#include "guidance/guidance_indi.h"
#include "subsystems/navigation/waypoints.h"
#include "subsystems/datalink/datalink.h"
#include "modules/sensors/temp_adc.h"
#include "subsystems/radio_control.h"
#include "modules/vision_outback/vision_outback.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="0" y="0"/>
    <waypoint name="MB1" x="1.2" y="-1342.1"/>
    <waypoint name="MB2" x="-1392.4" y="-55.5"/>
    <waypoint name="MB3" x="363.8" y="1258.5"/>
    <waypoint name="MB4" x="1401.8" y="-71.5"/>
    <waypoint alt="40.0" name="BASE" x="19.7" y="23.7"/>
    <waypoint alt="40.0" name="JOE" x="12.8" y="30.8"/>
    <waypoint alt="40.0" name="JOE_reported" x="-20.7" y="50.0"/>
    <waypoint name="JOE_found" x="112.7" y="-4.1"/>
    <waypoint name="TD_mrk" x="123.6" y="13.2"/>
    <waypoint name="p1" x="-498.0" y="-203.9"/>
    <waypoint name="p2" x="-543.9" y="-57.8"/>
    <waypoint name="p3" x="40.5" y="73.9"/>
    <waypoint name="p4" x="7.4" y="195.5"/>
    <waypoint alt="40.0" name="TD" x="27.6" y="16.6"/>
    <waypoint name="CLIMB" x="40.0" y="33.2"/>
    <waypoint name="STDBY" x="-51.2" y="32.3"/>
    <waypoint name="HOVER" x="101.6" y="-21.6"/>
  </waypoints>
  <sectors>
    <sector color="yellow" name="MissionBoundary" type="dynamic">
      <corner name="MB1"/>
      <corner name="MB2"/>
      <corner name="MB3"/>
      <corner name="MB4"/>
    </sector>
  </sectors>
  <modules>
    <module name="nav" type="heli_spinup"/>
    <module name="nav" type="hybrid_heading"/>
  </modules>
  <exceptions>
    <exception cond="(!InsideMissionBoundary(GetPosX(), GetPosY()) &&
      !(nav_block == IndexOfBlock('waitGps')) &&
      !(nav_block == IndexOfBlock('geoInit')) &&
      !(nav_block == IndexOfBlock('holdingPoint')) &&
      !(nav_block == IndexOfBlock('failsafe'))
      )" deroute="failsafe"/>
    <exception cond="(!GpsFixValid() &&
      !(nav_block == IndexOfBlock('GpsLoss')) &&
      !(nav_block == IndexOfBlock('Descent_noGPS')) &&
      !(nav_block == IndexOfBlock('Descent_slow_noGPS')) &&
      !(nav_block == IndexOfBlock('landing_moment_noGPS')) &&
      !(nav_block == IndexOfBlock('landed')) &&
      !(nav_block == IndexOfBlock('failsafe')) &&
      (autopilot_in_flight() == true) )" deroute="GpsLoss"/>
    <exception cond="(datalink_time > 180 &&
      !(nav_block == IndexOfBlock('DatalinkLoss')) &&
      !(nav_block == IndexOfBlock('failsafe')) &&
      !(nav_block == IndexOfBlock('landed')) &&
      (autopilot_in_flight() == true) )" deroute="DatalinkLoss"/>
  </exceptions>
  <blocks>
    <block group="init" name="waitGps">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <manual pitch="0" roll="0" throttle="0" until="TRUE" vmode="throttle" yaw="0"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block group="init" name="geoInit">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <call_once fun="NavSetAltitudeReferenceHere()"/>
    </block>
    <block group="init" name="holdingPoint">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block group="takeoff_base" name="base_startEngine" strip_button="Start_Engine">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <call_once fun="nav_heli_spinup_setup(2, 0.07)"/>
      <call_once fun="NavResurrect()"/>
      <call fun="nav_heli_spinup_run()"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>1" vmode="throttle"/>
    </block>
    <block group="takeoff_base" name="base_holdAttitude">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="1200 > throttle_curve.rpm_meas" deroute="base_startEngine"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <call_once fun="nav_set_heading_current()"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>0" vmode="throttle"/>
    </block>
    <block group="takeoff_base" name="base_takeOff">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="stateGetPositionEnu_f()->z > 4.0" deroute="base_climb"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude pitch="0" roll="0" throttle="0.78" until="stage_time>2" vmode="throttle"/>
    </block>
    <block group="takeoff_base" name="base_climb">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="stateGetPositionEnu_f()->z > 50.0" deroute="transition_to_joe"/>
      <call_once fun="NavSetWaypointHere(WP_CLIMB)"/>
      <call_once fun="GuidanceVSetRef(stateGetPositionNed_i()->z - POS_BFP_OF_REAL(5.0), -SPEED_BFP_OF_REAL(3.0), 0)"/>
      <stay climb="nav_climb_vspeed" until="stateGetPositionEnu_f()->z > 20.0" vmode="climb" wp="CLIMB"/>
      <call_once fun="nav_hybrid_heading_init()"/>
      <stay climb="nav_climb_vspeed" pre_call="nav_hybrid_heading_set()" vmode="climb" wp="CLIMB"/>
    </block>
    <block group="fly_to_joe" name="transition_to_joe">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_set_heading_towards_waypoint(WP_p1)"/>
      <stay until="stage_time>2" wp="p1"/>
    </block>
    <block group="fly_to_joe" name="route_to_joe">
      <set value="HB_FORWARD" var="dc_hybrid_mode"/>
      <go from="p4" hmode="route" wp="p1"/>
      <go from="p1" hmode="route" wp="p2"/>
      <go from="p2" hmode="route" wp="p3"/>
      <go from="p3" hmode="route" wp="p4"/>
      <deroute block="route_to_joe"/>
    </block>
    <block group="fly_to_joe" name="transition_at_joe">
      <set value="HB_FORWARD" var="dc_hybrid_mode"/>
      <go wp="JOE"/>
    </block>
    <block group="land_joe" name="joe_land">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="enableVisionDescent(true)"/>
      <stay until="stage_time>4" wp="JOE"/>
    </block>
    <block group="land_joe" name="joe_descend">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="12.0 > stateGetPositionEnu_f()->z" deroute="joe_descendSlow"/>
      <call_once fun="nav_hybrid_heading_init()"/>
      <call_once fun="GuidanceVSetRef(stateGetPositionNed_i()->z, 0, 0)"/>
      <stay climb="nav_descend_vspeed" pre_call="nav_hybrid_heading_set()" vmode="climb" wp="JOE"/>
    </block>
    <block group="land_joe" name="joe_descendSlow">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="het_moment" deroute="joe_moment"/>
      <exception cond="vision_timeout " deroute="Standby"/>
      <exception cond="!vision_outback_enable_landing " deroute="Standby"/>
      <stay climb="-0.5" vmode="climb" wp="JOE"/>
    </block>
    <block group="land_joe" name="joe_moment">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude pitch="0" roll="0" throttle="0.5" until="stage_time>0" vmode="throttle"/>
      <deroute block="joe_landed"/>
    </block>
    <block group="land_joe" name="joe_landed">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <call_once fun="enableVisionDescent(false)"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>2" vmode="throttle"/>
    </block>
    <block group="wait_joe" name="joe_inert">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="opa_controller_ap_disarm(true)"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>2" vmode="throttle"/>
      <attitude pitch="0" roll="0" throttle="0" until="electrical.vsupply>190" vmode="throttle"/>
    </block>
    <block group="wait_joe" name="joe_wait">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <attitude pitch="0" roll="0" throttle="0" until="(stage_time>60) && GpsFixValid()" vmode="throttle"/>
    </block>
    <block group="takeoff_joe" name="joe_startEngine">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <call_once fun="nav_heli_spinup_setup(2, 0.07)"/>
      <call_once fun="NavResurrect()"/>
      <call fun="nav_heli_spinup_run()"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>1" vmode="throttle"/>
    </block>
    <block group="takeoff_joe" name="joe_holdAttitude">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="1200 > throttle_curve.rpm_meas" deroute="joe_takeOff"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <call_once fun="nav_set_heading_current()"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>0" vmode="throttle"/>
    </block>
    <block group="takeoff_joe" name="joe_takeOff">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="stateGetPositionEnu_f()->z > 4.0" deroute="joe_climb"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude pitch="0" roll="0" throttle="0.78" until="stage_time>2" vmode="throttle"/>
    </block>
    <block group="takeoff_joe" name="joe_climb">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="stateGetPositionEnu_f()->z > 50.0" deroute="transition_to_base"/>
      <call_once fun="NavSetWaypointHere(WP_CLIMB)"/>
      <call_once fun="GuidanceVSetRef(stateGetPositionNed_i()->z - POS_BFP_OF_REAL(5.0), -SPEED_BFP_OF_REAL(3.0), 0)"/>
      <stay climb="nav_climb_vspeed" until="stateGetPositionEnu_f()->z > 20.0" vmode="climb" wp="CLIMB"/>
      <call_once fun="nav_hybrid_heading_init()"/>
      <stay climb="nav_climb_vspeed" pre_call="nav_hybrid_heading_set()" vmode="climb" wp="CLIMB"/>
    </block>
    <block group="fly_to_base" name="transition_to_base">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_set_heading_towards_waypoint(WP_p1)"/>
      <stay until="stage_time>2" wp="p1"/>
    </block>
    <block group="fly_to_base" name="route_to_base">
      <set value="HB_FORWARD" var="dc_hybrid_mode"/>
      <go from="p4" hmode="route" wp="p1"/>
      <go from="p1" hmode="route" wp="p2"/>
      <go from="p2" hmode="route" wp="p3"/>
      <go from="p3" hmode="route" wp="p4"/>
      <deroute block="route_to_base"/>
    </block>
    <block group="fly_to_base" name="transition_at_base">
      <set value="HB_FORWARD" var="dc_hybrid_mode"/>
      <go wp="BASE"/>
    </block>
    <block group="land_base" name="base_land">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="enableVisionDescent(true)"/>
      <stay until="stage_time>4" wp="BASE"/>
    </block>
    <block group="land_base" name="base_descend">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="12.0 > stateGetPositionEnu_f()->z" deroute="base_descendSlow"/>
      <call_once fun="nav_hybrid_heading_init()"/>
      <call_once fun="GuidanceVSetRef(stateGetPositionNed_i()->z, 0, 0)"/>
      <stay climb="nav_descend_vspeed" pre_call="nav_hybrid_heading_set()" vmode="climb" wp="BASE"/>
    </block>
    <block group="land_base" name="base_descendSlow">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="het_moment" deroute="base_moment"/>
      <exception cond="vision_timeout " deroute="Standby"/>
      <exception cond="!vision_outback_enable_landing " deroute="Standby"/>
      <stay climb="-0.5" vmode="climb" wp="BASE"/>
    </block>
    <block group="land_base" name="base_moment">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude pitch="0" roll="0" throttle="0.5" until="stage_time>0" vmode="throttle"/>
      <deroute block="base_landed"/>
    </block>
    <block group="land_base" name="base_landed">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <call_once fun="enableVisionDescent(false)"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>2" vmode="throttle"/>
    </block>
    <block group="wait_base" name="base_inert">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="opa_controller_ap_disarm(true)"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>2" vmode="throttle"/>
      <attitude pitch="0" roll="0" throttle="0" until="electrical.vsupply>190" vmode="throttle"/>
    </block>
    <block group="safe" name="Standby" strip_button="Standby">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <stay wp="STDBY"/>
    </block>
    <block group="safe" name="StandbyAutoHeading" strip_button="Standby90">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_hybrid_heading_init()"/>
      <stay pre_call="nav_hybrid_heading_set()" wp="STDBY"/>
    </block>
    <block group="safe" name="Hover_here" strip_button="HoverHere">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="waypoint_set_alt(WP_HOVER,stateGetPositionEnu_f()->z)"/>
      <call_once fun="NavSetWaypointHere(WP_HOVER)"/>
      <stay wp="HOVER"/>
    </block>
    <block group="safe" name="LandHere" strip_button="LandHere">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="12.0 > stateGetPositionEnu_f()->z" deroute="LandHereSlow"/>
      <call_once fun="enableVisionDescent(true)"/>
      <call_once fun="nav_set_heading_current()"/>
      <call_once fun="NavSetWaypointHere(WP_TD)"/>
      <call_once fun="GuidanceVSetRef(stateGetPositionNed_i()->z, 0, 0)"/>
      <stay climb="nav_descend_vspeed" vmode="climb" wp="TD"/>
    </block>
    <block group="safe" name="LandHereSlow">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="enableVisionDescent(true)"/>
      <exception cond="het_moment" deroute="landing_moment"/>
      <exception cond="vision_timeout " deroute="Standby"/>
      <exception cond="!vision_outback_enable_landing " deroute="Standby"/>
      <stay climb="-0.5" vmode="climb" wp="TD"/>
    </block>
    <block group="safe" name="landing_moment">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude pitch="0" roll="0" throttle="0.5" until="stage_time>0" vmode="throttle"/>
      <deroute block="landed"/>
    </block>
    <block group="safe" name="landed">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <call_once fun="enableVisionDescent(false)"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>2" vmode="throttle"/>
    </block>
    <block group="autosafe" name="GpsLoss">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="2.0 > stateGetPositionEnu_f()->z" deroute="landing_moment_noGPS"/>
      <exception cond="GpsFixValid() && (20.0 > stateGetPositionEnu_f()->z)" deroute="Hover_here"/>
      <exception cond="block_time>30" deroute="Descent_noGPS"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <call_once fun="waypoint_set_alt(WP_HOVER,stateGetPositionEnu_f()->z)"/>
      <call_once fun="NavSetWaypointHere(WP_HOVER)"/>
      <stay until="stage_time>10" wp="HOVER"/>
      <attitude climb="nav_descend_vspeed" pitch="0" roll="0" until="GpsFixValid() && (stateGetPositionEnu_f()->z > 20.0)" vmode="climb"/>
      <deroute block="Standby"/>
    </block>
    <block group="autosafe" name="DatalinkLoss">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="block_time>120" deroute="Standby"/>
      <exception cond="get_temp(1) > 100" deroute="Standby"/>
      <call_once fun="NavSetWaypointHere(WP_HOVER)"/>
      <call_once fun="waypoint_set_alt(WP_HOVER,stateGetPositionEnu_f()->z + 20)"/>
      <stay until="60 > datalink_time" wp="HOVER"/>
      <stay wp="HOVER"/>
    </block>
    <block group="autosafe" name="failsafe">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="NavKillThrottle()"/>
      <call_once fun="NavSetFailsafe()"/>
      <call_once fun="INTERMCU_SET_CMD_STATUS(INTERMCU_CMD_FAILSAFE)"/>
      <call_once fun="nav_throttle_curve_set(0)"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block group="autosafe" name="Descent_noGPS">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <exception cond="10.0 > stateGetPositionEnu_f()->z" deroute="Descent_slow_noGPS"/>
      <call_once fun="nav_set_heading_current()"/>
      <call_once fun="NavSetWaypointHere(WP_TD)"/>
      <call_once fun="GuidanceVSetRef(stateGetPositionNed_i()->z, 0, 0)"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude climb="nav_descend_vspeed" pitch="0" roll="0" vmode="climb"/>
    </block>
    <block group="autosafe" name="Descent_slow_noGPS">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude climb="-0.5" pitch="0" roll="0" vmode="climb"/>
    </block>
    <block group="autosafe" name="landing_moment_noGPS">
      <set value="HB_HOVER" var="dc_hybrid_mode"/>
      <call_once fun="nav_throttle_curve_set(1)"/>
      <attitude pitch="0" roll="0" throttle="0.5" until="stage_time>0" vmode="throttle"/>
      <deroute block="landed"/>
    </block>
  </blocks>
</flight_plan>
