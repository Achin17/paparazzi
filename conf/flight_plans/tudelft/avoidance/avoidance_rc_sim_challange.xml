  <blocks>
    <block name="Wait GPS">
      <while cond="!GpsFixValid()"/>
      <set value="1" var="autopilot.kill_throttle"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <call_once fun="NavSetAltitudeReferenceHere()"/>
      <set value="TRUE" var="air_data.calc_qnh_once"/>
    </block>
    <block name="Holding point">
      <set value="1" var="autopilot.kill_throttle"/>
      <attitude roll="0" throttle="0" vmode="throttle"/>
    </block>
    <block key="r" name="Start Engine">
      <call_once fun="NavResurrect()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
      <call_once fun="NavResurrect()"/>
    </block>
    <block key="t" name="Takeoff" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="GetPosAlt() > 1.0" deroute="Standby"/>
      <call_once fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay climb="nav_climb_vspeed" vmode="climb" wp="CLIMB"/>
      <deroute block="BASE to FP1"/>
    </block>
    <block key="s" name="Standby" strip_button="Standby" strip_icon="home.png">
      <circle radius="nav_radius" wp="STDBY"/>
    </block>
    <block key="s" name="ResetMission">
      <call_once fun="mission_path_reset()"/>
      <circle radius="nav_radius" wp="STDBY"/>
    </block>
    <block name="RUN MISSION To JOE">
      <call fun="mission_path_run(10000)"/>
      <deroute block="FP7 to JOE"/>
    </block>
    <block name="BASE to FP1">
	  <go from="FP1" hmode="route" wp="FP2"/>
    </block>
    <block name="FP1 to FP2">
      <go from="FP1" hmode="route" wp="FP2"/>
    </block>
    <block name="FP2 to FP3">
      <go from="FP2" hmode="route" wp="FP3"/>
    </block>
    <block name="FP3 to FP4">
      <go from="FP3" hmode="route" wp="FP4"/>
    </block>
    <block name="FP4 to FP5">
      <go from="FP4" hmode="route" wp="FP5"/>
    </block>
    <block name="FP5 to FP6">
      <go from="FP5" hmode="route" wp="FP6"/>
    </block>
    <block name="FP6 to FP7">
      <go from="FP6" hmode="route" wp="FP7"/>
    </block>
    <block name="FP7 to JOE">
      <go from="FP7" hmode="route" wp="JOE_reported"/>
    </block>
    <block name="Land JOE">
      <call_once fun="NavSetWaypointHere(WP_TD)"/>
      <go wp="TD"/>
    </block>
    <block name="Flare JOE">
      <exception cond="NavDetectGround()" deroute="Holding point"/>
      <exception cond="!nav_is_in_flight()" deroute="Landed"/>
      <exception cond="0.10 > GetPosAlt()" deroute="Landed"/>
      <call_once fun="NavStartDetectGround()"/>
      <stay climb="nav_descend_vspeed" vmode="climb" wp="TD"/>
    </block>
    <block name="Landed JOE">
      <call_once fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Hold JOE">
      <set value="1" var="autopilot.kill_throttle"/>
      <attitude roll="0" throttle="0" vmode="throttle"/>
    </block>
    <block name="Start Engine JOE">
      <call_once fun="NavResurrect()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
      <call_once fun="NavResurrect()"/>
    </block>
    <block name="Takeoff JOE" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="GetPosAlt() > 1.0" deroute="Standby"/>
      <call_once fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay climb="nav_climb_vspeed" vmode="climb" wp="CLIMB"/>
    </block>
    <block name="JOE to FP7">
      <go from="JOE_reported" hmode="route" wp="FP7"/>
    </block>
    <block name="RUN MISSION FROM JOE">
      <call fun="mission_path_run(20000)"/>
      <deroute block="FP1 to BASE"/>
    </block>
    <block name="FP7 to FP6">
      <go from="FP7" hmode="route" wp="FP6"/>
    </block>
    <block name="FP6 to FP5">
      <go from="FP6" hmode="route" wp="FP5"/>
    </block>
    <block name="FP5 to FP4">
      <go from="FP5" hmode="route" wp="FP4"/>
    </block>
    <block name="FP4 to FP3">
      <go from="FP4" hmode="route" wp="FP3"/>
    </block>
    <block name="FP3 to FP2">
      <go from="FP3" hmode="route" wp="FP2"/>
    </block>
    <block name="FP2 to FP1">
      <go from="FP2" hmode="route" wp="FP1"/>
    </block>
    <block name="FP1 to BASE">
      <go from="FP1" hmode="route" wp="BASE"/>
      <deroute block="Land here"/>
    </block>
    <block key="l" name="Land here" strip_button="Land Here" strip_icon="land-right.png">
      <call_once fun="NavSetWaypointHere(WP_TD)"/>
      <go wp="TD"/>
      <deroute block="Flare"/>
    </block>
    <block name="Land">
      <go wp="TD"/>
      <deroute block="Flare"/>
    </block>
    <block name="Flare">
      <exception cond="NavDetectGround()" deroute="Holding point"/>
      <exception cond="!nav_is_in_flight()" deroute="Landed"/>
      <exception cond="0.10 > GetPosAlt()" deroute="Landed"/>
      <call_once fun="NavStartDetectGround()"/>
      <stay climb="nav_descend_vspeed" vmode="climb" wp="TD"/>
    </block>
    <block name="Landed">
      <call_once fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
